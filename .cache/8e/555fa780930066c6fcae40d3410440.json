riginal":{"line":84,"column":16},"generated":{"line":103,"column":18}},{"source":"components/CalendarYear.ts","name":null,"original":{"line":84,"column":4},"generated":{"line":103,"column":22}},{"source":"components/CalendarYear.ts","name":null,"original":{"line":85,"column":3},"generated":{"line":104,"column":0}},{"source":"components/CalendarYear.ts","name":null,"original":{"line":3,"column":34},"generated":{"line":108,"column":19}},{"source":"components/CalendarYear.ts","name":null,"original":{"line":98,"column":0},"generated":{"line":108,"column":34}}],"sources":{"components/CalendarYear.ts":"import './CalendarMonth.ts';\n\nexport class CalendarYear extends HTMLElement {\n\n  year : number;\n\n  constructor() {\n    super();\n    const date = new Date();\n    this.year = Number(this.getAttribute('year')) || date.getFullYear();\n  }\n\n  connectedCallback() {\n    this.innerHTML = `\n      <style>\n        .months {\n          display: grid;\n          grid-template-columns: repeat(3, 1fr);\n          grid-template-rows: repeat(4, 1fr);\n          justify-items: center;\n        }\n        @media screen and (max-width: 1750px) {\n          .months {\n            grid-template-columns: repeat(2, 1fr);\n            grid-template-rows: repeat(6, 1fr);\n          }\n        }\n        @media screen and (max-width: 1200px) {\n          .months {\n            grid-template-columns: repeat(12, 1fr);\n            grid-template-columns: 1fr;\n          }\n        }\n        .year > header {\n          margin-bottom: 5px;\n        }\n        .prev, .next {\n          background: transparent;\n          border: 0;\n          font-size: 18px;\n          color: gold;\n          cursor: pointer;\n        }\n      </style>\n      <div class=\"year\">\n        <header>\n          <button class=\"prev\">Prev</button>\n          ${this.year}\n          <button class=\"next\">Next</button>\n        </header>\n        <div class=\"months\">\n          ${this.getMonths()}\n        </div>\n      </div>\n    `;\n\n    this.querySelector('.prev')!.addEventListener('click', () => this.setNewYear(this.year - 1));\n    this.querySelector('.next')!.addEventListener('click', () => this.setNewYear(this.year + 1));\n  }\n\n  setNewYear(year : number) {\n    const parent = this.parentElement;\n    const element = document.createElement('calendar-year');\n    element.setAttribute('year', year.toString());\n    this.remove();\n    parent!.append(element);\n  }\n\n  static get observedAttributes() {\n    return ['year'];\n  }\n\n  attributeChangedCallback(name : string, oldValue : string, newValue : string) {\n    if (name === 'year') {\n      this.year = Number(newValue);\n    }\n  }\n\n  static set year(y : number) {\n    this.year = y;\n  }\n\n  static get year() {\n    return this.year;\n  }\n\n  getMonths() {\n    const months = [];\n    for (let i = 1; i <= 12; i++) {\n      months.push(`<calendar-month month=\"${i}\" year=\"${this.year}\"></calendar-month>`);\n    }\n\n    return months.join('');\n  }\n\n}\n\n// customElements.define('calendar-year', CalendarYear);\n"},"lineCount":null}},"error":null,"hash":"88f97c7427b093a04d13d434668c33f1","cacheData":{"env":{}}}